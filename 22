#include <stdio.h>
int main(){
	int numero;
	printf("Informe um numero entre 1 e 10 :");
	fflush(stdout);
	scanf("%d", &numero);

	while (numero > 10 || numero < 1){
		printf("Informe um numero entre 1 e 10:");
		fflush(stdout);
		scanf("%d", &numero);
	}
	for (int i=1; i<=10;i++){
		printf("%d x %d = %d\n", numero, i,(numero * i));

	}


}
