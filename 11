#include <stdio.h>
int main(){
	float p, m;
	char e[8] = "excedente";


	printf("Informe  o peso dos peixes: ");
	fflush(stdout);
	scanf("%f", &p);

	if( p > 50){
		m = (p - 50) * 4.00;
		printf("A multa Ã© de: %.2f ", m);
	}
	else{
		m = 0;
		e [0] = 0;
		printf("O peso esta dentro do permitido!");

	}

}
