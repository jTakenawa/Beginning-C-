#include <stdio.h>
int main(){
int  vetores[5], pares[5], cont_p = 0;
for(int i=0; i < 5 ; i++){
printf("Insira um numero:");
fflush(stdout);
scanf("%d", &vetores[i]);
if(vetores[i] %2 == 0){
	if(vetores[i] > 0){
		pares[cont_p] = vetores[i];
		cont_p = cont_p + 1;
	}
}
}
for(int i; i < cont_p; i++){
	printf("%d\n", pares[i]);
}
}
