#include <stdio.h>
int main (){
	int vetor[10],vetor1[10],vetor2[10];

	for(int i = 0;i< 10; i++){
		printf("Informe um valor para o primeiro vetor: ");
		fflush(stdout);
		scanf("%d", &vetor[i]);
		printf("Informe um valor para o segundo vetor: ");
		fflush(stdout);
		scanf("%d", &vetor1[i]);
		vetor2 [i]= vetor[i] + vetor1[i];
	}
	for(int i = 0; i < 10; i++){
		printf("%.2d\n", vetor2[i]);
	}
}
